{
	"scripts": {
		"dev": "npm run server:dev",
		"prod": "npm run lint && npm run build && npm run server:prod",
		"build": "vite build",
		"test": "vitest --run --silent --reporter=verbose",
		"coverage": "vitest --coverage",
		"server": "node --loader ts-node/esm ./server/index.ts",
		"server:dev": "npm run server",
		"server:prod": "cross-env NODE_ENV=production npm run server",
		"lint": "eslint . --max-warnings 0",
		"format": "prettier --write .",
		"prepare": "husky"
	},
	"dependencies": {
		"@emotion/react": "^11.13.3",
		"@emotion/styled": "^11.13.0",
		"@mui/material": "^6.1.2",
		"@reduxjs/toolkit": "^2.2.7",
		"@testing-library/jest-dom": "^6.6.1",
		"@testing-library/react": "^16.0.1",
		"@types/compression": "^1.7.2",
		"@types/express": "^4.17.17",
		"@types/node": "^20.4.10",
		"@types/react": "^18.2.20",
		"@types/react-dom": "^18.2.7",
		"@types/uuid": "^10.0.0",
		"@typescript-eslint/eslint-plugin": "^6.3.0",
		"@typescript-eslint/parser": "^6.3.0",
		"@vitejs/plugin-react": "^4.0.4",
		"compression": "^1.7.4",
		"cross-env": "^7.0.3",
		"eslint": "^8.47.0",
		"eslint-plugin-react": "^7.33.1",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.3",
		"express": "^4.18.2",
		"jsdom": "^25.0.1",
		"react": "^18.2.0",
		"react-aria-components": "^1.4.0",
		"react-dom": "^18.2.0",
		"react-redux": "^9.1.2",
		"sirv": "^2.0.3",
		"ts-node": "^10.9.2",
		"typescript": "^5.1.6",
		"uuid": "^10.0.0",
		"vite": "^4.4.9",
		"vite-plugin-ssr": "^0.4.142"
	},
	"type": "module",
	"devDependencies": {
		"husky": "^9.1.6",
		"prettier": "^3.3.3",
		"vitest": "^2.1.3"
	}
}
