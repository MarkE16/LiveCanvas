{
  "version": 2,
  "builds": [
    {
      "src": "api/ssr.js",
      "use": "@vercel/node"
    },
    {
      "src": "dist/client/**",
      "use": "@vercel/static"
    },
    {
      "src": "dist/server/**",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(?!assets/).*",
      "dest": "/api/ssr.js"
    }
  ]
}